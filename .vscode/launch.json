  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Scrape Data",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\index.mjs"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Scrape Icons",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\scrape_icons.mjs"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Clean Data",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\clean_data.mjs"
    }
  ]
}